<%-include("./partials/header")-%>

<body>
  <body class="bg-primary dark:bg-primarydark w-full h-full">
    <main class="h-full w-auto flex justify-center">
      <div class="h-full w-full sm:w-5/6 flex flex-col">
        <div
          class="user-information rounded flex flex-row w-full h-2/5 mt-8 bg-[url('https://w0.peakpx.com/wallpaper/805/27/HD-wallpaper-2d-pixel-art-medieval-background-pack-2d-unity-asset-store.jpg')] bg-cover bg-center"
        >
          <!-- User Information -->
          <div class="user-information-person flex w-2/5 items-center">
            <div
              class="user-content bg-red-400 rounded-b-lg flex w-full h-4/5 m-2"
            ></div>
          </div>
          <div
            class="user-profile-attribute w-2/4 flex justify-center text-center items-center"
          >
            <div class="user-text">
              <img
                class="w-48 h-44 border border-cyan-100 rounded-full"
                src="https://mir-s3-cdn-cf.behance.net/projects/202/6467b333840174.Y3JvcCw5ODYsNzcyLDExOSww.jpg"
                alt=""
              />
              <h1
                class="text-2xl text-white bg-stone-700 border border-double border-tertiarydark rounded"
              >
                Nome do Usuário
              </h1>
              <h2 class="text-xl text-white">Email do Usuário</h2>
            </div>
          </div>
          <div class="user-information-person flex w-2/5 items-center">
            <div
              class="user-content bg-green-400 rounded-b-lg flex w-full h-4/5 m-2"
            ></div>
          </div>
        </div>
        <div class="user-menu flex w-full h-auto"></div>
      </div>
    </main>

    <script
      data-cfasync="false"
      src="/cdn-cgi/scripts/5c5dd728/cloudflare-static/email-decode.min.js"
    ></script>
    <script src="https://code.jquery.com/jquery-1.10.2.min.js"></script>
  </body>
</body>
